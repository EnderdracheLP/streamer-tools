{
  "id": "discord-presence",
  "name": "Discord Presence",
  "version": "0.1.2",
  "author": "TheProff#6700",
  "description": [
    "Integrates with an application by MadMagic to allow you to see your current position in a song and what song you're playing in discord while playing beat saber."
  ],
  "category": "Gameplay",
  "gameVersion": "1.12.1",
  "platform": "Quest",
  "components": [
    {
      "type": "HookMod",
      "installAction": {
        "installLibraryFile": "libdiscord-presence_0_1_2.so"
      },
      "uninstallAction": {
        "removeLibraryFile": "libdiscord-presence_0_1_2.so"
      }
    },
    {
      "type": "HookModLibrary",
      "installAction": {
        "installLibraryFile": "libbeatsaber-hook_0_7_4.so"
      }
    },
    {
      "type": "FileCopyMod",
      "sourceFileName": "module.json",
      "targetRootedPathAndFileName": "/sdcard/OQRPC Modules/beat_saber.json"
    }
  ]
}
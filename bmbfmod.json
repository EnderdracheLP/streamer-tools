{
    "id": "discord-presence",
    "name": "Discord Presence",
    "author": "TheProff#6700",
    "description": [
        "Integrates with an application by MadMagic to allow you to see your current position in a song and what song you're playing in discord while playing beat saber."
    ],
    "category": "Gameplay",
    "gameVersion": "1.11.0",
    "version": "0.1.0",
    "platform": "Quest",
    "components": [
        {
            "type": "HookMod",
            "installAction": {
                "installLibraryFile": "libdiscord-presence.so"
            },
            "uninstallAction": {
                "removeLibraryFile": "libdiscord-presence.so"
            }
        },
        {
            "type": "HookModLibrary",
            "installAction": {
                "installLibraryFile": "libbeatsaber-hook_2019_0_4_3.so"
            }
        },
        {
            "type": "FileCopyMod",
            "sourceFileName": "module.json",
            "targetRootedPathAndFileName": "/sdcard/OQRPC Modules/beat_saber.json"
        },
        {
            "type": "FileCopyMod",
            "sourceFileName": "config.json",
            "targetRootedPathAndFileName": "/sdcard/Android/data/com.beatgames.beatsaber/files/mod_cfgs/discord-presence.json"
        }
    ]
}

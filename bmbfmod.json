{
  "id": "discord-presence",
  "name": "Discord Presence",
  "version": "0.1.3",
  "icon": "cover.png",
  "coverImageFilename": "cover.png",
  "author": "TheProff#6700",
  "description": [
    "Integrates with an application by MadMagic to allow people to see which song you're playing, how far through you are, etc. on Discord while playing beat saber on quest. See https://github.com/Lauriethefish/Quest-Discord-Presence for install instructions."
  ],
  "category": "Gameplay",
  "gameVersion": "1.13.0",
  "platform": "Quest",
  "components": [
    {
      "type": "HookMod",
      "installAction": {
        "installLibraryFile": "libdiscord-presence_0_1_3.so"
      },
      "uninstallAction": {
        "removeLibraryFile": "libdiscord-presence_0_1_3.so"
      }
    },
    {
      "type": "HookModLibrary",
      "installAction": {
        "installLibraryFile": "libbeatsaber-hook_0_8_4.so"
      }
    },
    {
      "type": "FileCopyMod",
      "sourceFileName": "module.json",
      "targetRootedPathAndFileName": "/sdcard/OQRPC Modules/beat_saber.json"
    }
  ]
}